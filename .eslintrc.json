{
    "env": {
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-new": "off",
        "no-prototype-builtins": "off",
        "no-restricted-syntax": "off",
        "max-classes-per-file": "off",
        "no-console": "off",
        "import/prefer-default-export": "off",
        "no-useless-constructor": "off",
        "no-underscore-dangle": "off",
        "no-await-in-loop": "off",
        "consistent-return": "off",
        "indent": [
            "error",
            4
        ],
        "linebreak-style": "off",
        "max-len": [
            "error",
            {
                "code": 150
            }
        ]
    }
}